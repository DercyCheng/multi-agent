# Multi-Agent Platform Environment Configuration
# Copy this file to .env and update the values according to your environment

# =============================================================================
# CORE CONFIGURATION
# =============================================================================

# Environment (development, staging, production)
ENVIRONMENT=development

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production-minimum-32-characters

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================

# PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=multiagent
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_SSLMODE=disable

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Qdrant Vector Database
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_GRPC_PORT=6334
QDRANT_API_KEY=

# =============================================================================
# LLM PROVIDER CONFIGURATION
# =============================================================================

# OpenAI
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_BASE_URL=https://api.openai.com/v1

# Anthropic
ANTHROPIC_API_KEY=your-anthropic-api-key-here
ANTHROPIC_BASE_URL=https://api.anthropic.com

# Google/Gemini
GOOGLE_API_KEY=your-google-api-key-here

# Azure OpenAI (if using)
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_API_VERSION=2024-02-01

# =============================================================================
# SERVICE PORTS
# =============================================================================

# API Gateway
GATEWAY_PORT=8080
GATEWAY_METRICS_PORT=9090

# Orchestrator Service
ORCHESTRATOR_PORT=8081
ORCHESTRATOR_GRPC_PORT=50052

# Agent Core Service
AGENT_CORE_PORT=8082
AGENT_CORE_GRPC_PORT=50051

# LLM Service
LLM_SERVICE_PORT=8000

# =============================================================================
# EXTERNAL SERVICES
# =============================================================================

# Temporal Workflow Engine
TEMPORAL_HOST=localhost
TEMPORAL_PORT=7233
TEMPORAL_NAMESPACE=multi-agent

# Prometheus Monitoring
PROMETHEUS_PORT=9090
PROMETHEUS_SCRAPE_INTERVAL=15s

# Jaeger Tracing
JAEGER_ENDPOINT=http://localhost:14268/api/traces
JAEGER_SERVICE_NAME=multi-agent

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================

# Authentication
AUTH_ENABLED=true
SKIP_AUTH=false
ACCESS_TOKEN_EXPIRY=30m
REFRESH_TOKEN_EXPIRY=168h
API_KEY_RATE_LIMIT=1000

# Password Security
PASSWORD_HASH_COST=12

# Rate Limiting
RATE_LIMIT_ENABLED=true
REQUESTS_PER_MINUTE=60
RATE_LIMIT_BURST=10

# CORS Configuration
CORS_ENABLED=true
CORS_ALLOW_ORIGINS=*
CORS_ALLOW_CREDENTIALS=true

# TLS/SSL (for production)
TLS_ENABLED=false
TLS_CERT_FILE=
TLS_KEY_FILE=
TLS_CA_FILE=

# =============================================================================
# ENFORCEMENT & LIMITS
# =============================================================================

# Agent Core Enforcement
ENFORCE_TIMEOUT_SECONDS=30
ENFORCE_MAX_TOKENS=100000
ENFORCE_RATE_LIMIT_PER_KEY=1000
ENFORCE_CIRCUIT_BREAKER_MAX_FAILURES=5

# WASI Sandbox Limits
WASI_MEMORY_LIMIT_MB=256
WASI_CPU_TIMEOUT_SECONDS=30
WASI_NETWORK_ACCESS=false

# Memory Pool Configuration
MEMORY_POOL_INITIAL_SIZE_MB=64
MEMORY_POOL_MAX_SIZE_MB=512
MEMORY_POOL_GC_THRESHOLD=0.8

# =============================================================================
# LLM CONFIGURATION
# =============================================================================

# Model Selection
DEFAULT_MODEL_TIER=medium
ENABLE_MODEL_FALLBACK=true

# Cost Controls
MAX_COST_PER_REQUEST=0.10
DAILY_BUDGET_USD=100.0
ALERT_THRESHOLD_PERCENT=80

# Prompt Caching
PROMPT_CACHE_ENABLED=true
PROMPT_CACHE_SIMILARITY_THRESHOLD=0.95
PROMPT_CACHE_TTL_SECONDS=3600
PROMPT_CACHE_MAX_SIZE=1000

# =============================================================================
# WORKFLOW CONFIGURATION
# =============================================================================

# Orchestrator Settings
MAX_PARALLEL_AGENTS=5
DEFAULT_WORKFLOW_TIMEOUT=300s
WORKFLOW_RETRY_ATTEMPTS=3

# Budget Management
DEFAULT_TOKEN_LIMIT=10000
BUDGET_DAILY_RESET=true
BUDGET_GRACE_PERIOD_PERCENT=10

# =============================================================================
# MCP TOOLS CONFIGURATION
# =============================================================================

# MCP Registry
MCP_REGISTER_TOKEN=your-mcp-admin-token-here
MCP_DEFAULT_TIMEOUT=30s
MCP_MAX_TOOLS_PER_AGENT=10

# External Tool APIs
WEATHER_API_KEY=your-weather-api-key-here
SEARCH_API_KEY=your-search-api-key-here
MAPS_API_KEY=your-maps-api-key-here

# =============================================================================
# MONITORING & OBSERVABILITY
# =============================================================================

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json
LOG_OUTPUT=stdout
LOG_FILE_PATH=/var/log/multi-agent.log
LOG_MAX_SIZE_MB=100
LOG_MAX_FILES=3
LOG_MAX_AGE_DAYS=7

# Metrics
METRICS_ENABLED=true
METRICS_PORT=9090
METRICS_PATH=/metrics

# Tracing
TRACING_ENABLED=true
TRACING_SAMPLER_TYPE=const
TRACING_SAMPLER_PARAM=1

# Health Checks
HEALTH_CHECK_INTERVAL=30s
DEEP_HEALTH_CHECK=false

# =============================================================================
# DEVELOPMENT SETTINGS
# =============================================================================

# Development Features
HOT_RELOAD=true
VERBOSE_LOGGING=true
DISABLE_AUTH_DEV=false
MOCK_LLM_RESPONSES=false
DEBUG_MODE=false

# Testing
TEST_DATABASE_URL=postgres://postgres:password@localhost:5432/multiagent_test
TEST_REDIS_URL=redis://localhost:6379/15

# =============================================================================
# PRODUCTION SETTINGS
# =============================================================================

# Production Security
FORCE_HTTPS=true
ENABLE_PROFILING=false
SECURE_COOKIES=true

# Performance
ENABLE_COMPRESSION=true
CONNECTION_POOL_SIZE=25
MAX_IDLE_CONNECTIONS=5
CONNECTION_MAX_LIFETIME=300s

# Backup and Recovery
BACKUP_ENABLED=false
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30

# =============================================================================
# FEATURE FLAGS
# =============================================================================

# Core Features
MULTI_TENANT_ENABLED=true
BLOCKCHAIN_INTEGRATION_ENABLED=false
ADVANCED_ANALYTICS_ENABLED=true
EXPERIMENTAL_FEATURES_ENABLED=false

# AI Features
CONTEXT_COMPRESSION_ENABLED=true
VECTOR_MEMORY_ENABLED=true
BELIEF_STATE_ENABLED=true
EXPLORATORY_UNDERSTANDING_ENABLED=true

# Integration Features
MCP_INTEGRATION_ENABLED=true
TEMPORAL_WORKFLOWS_ENABLED=true
OPA_POLICIES_ENABLED=true
CIRCUIT_BREAKERS_ENABLED=true

# =============================================================================
# CLOUD PROVIDER CONFIGURATION (Optional)
# =============================================================================

# AWS Configuration
AWS_REGION=us-west-2
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET=multi-agent-storage

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=
GOOGLE_APPLICATION_CREDENTIALS=

# Azure Configuration
AZURE_SUBSCRIPTION_ID=
AZURE_RESOURCE_GROUP=
AZURE_STORAGE_ACCOUNT=

# =============================================================================
# BLOCKCHAIN CONFIGURATION (Optional)
# =============================================================================

# Solana Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_WALLET_PRIVATE_KEY=
SOLANA_CLUSTER=mainnet-beta

# Ethereum Configuration (if using)
ETHEREUM_RPC_URL=
ETHEREUM_PRIVATE_KEY=
ETHEREUM_CONTRACT_ADDRESS=